    "@types/chai-http": "0.0.30",
    "@types/compression": "0.0.33",
    "@types/express": "^4.0.35",
    "@types/jasmine": "2.5.38",
    "@types/mongodb": "^2.1.39",
    "@types/mongoose": "^4.7.5",
    "@types/node": "^7.0.5",
    "chai": "^3.5.0",
    "chai-http": "^3.0.0",
