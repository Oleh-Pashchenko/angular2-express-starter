    "@angular/forms": "^2.4.7",
    "@angular/http": "^2.4.7",
    "@angular/platform-browser": "^2.4.7",
    "@angular/platform-browser-dynamic": "^2.4.7",
    "@angular/router": "^3.4.7",
    "angular-cli": "^1.0.0-beta.28.3",
    "body-parser": "^1.16.0",
    "compression": "^1.6.2",
    "core-js": "^2.4.1",
    "express": "^4.14.1",
    "mongodb": "^2.2.22",
    "mongoose": "^4.8.1",
    "rxjs": "^5.1.0",
    "tslint": "^4.4.2",
    "zone.js": "^0.7.6"
  },
  "devDependencies": {
