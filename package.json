{
  "name": "angular2-express-starter",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "angular-cli": {},
  "scripts": {
    "start": "concurrently --kill-others \"ng serve --aot=true --proxy-config proxy.conf.json\" \"npm run server:run\"",
    "build": "ng build --prod --sm=false --aot=true --output-path=dist/client && npm run server:build",
    "server:run": "tsc -p ./server && concurrently \"tsc -w -p ./server\" \"nodemon dist/server/bin/www.js\" ",
    "server:build": "tsc -p ./server",
    "test": "tsc -p ./server && mocha ./dist/server/tests/routes/main",
    "postinstall": "npm run build"
  },
  "author": "",
  "license": "ISC",
